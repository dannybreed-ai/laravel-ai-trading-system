# Laravel Framework Settings
APP_NAME="StocksandFi AI Trading System"
APP_ENV=local
APP_KEY=base64:YourAppKeyHere
APP_DEBUG=true
APP_URL=http://localhost

# Database Configuration
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=laravel_ai_trading
DB_USERNAME=your_username
DB_PASSWORD=your_password

# Mail Configuration
MAIL_MAILER=smtp
MAIL_HOST=mailpit
MAIL_PORT=1025
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

# Cache, Session, Queue, and Redis Settings
CACHE_DRIVER=file
SESSION_DRIVER=file
QUEUE_CONNECTION=sync
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

# AI Trading Specific Settings
TRADING_API_URL=https://api.trading.com
TRADING_API_KEY=your_trading_api_key
BLOCKCHAIN_API_KEY=your_blockchain_api_key
ENCRYPTION_KEY=your_encryption_key

# Financial System Settings
WELCOME_BONUS=10.00
MIN_DEPOSIT=100.00
MAX_DEPOSIT=1000000.00
MIN_WITHDRAWAL=10.00
MAX_WITHDRAWAL=1000000.00
DEPOSIT_FEE=0.00
WITHDRAWAL_FEE=2.00

# Referral System
REFERRAL_SYSTEM_ENABLED=true
REFERRAL_LEVELS=10
REFERRAL_COMMISSIONS="4,3,2,1,1,1,1,1,1,1"

# Security Settings
EMAIL_VERIFICATION_REQUIRED=true
KYC_VERIFICATION_REQUIRED=true
TWO_FACTOR_ENABLED=true
GOOGLE2FA_ENABLED=true

# Trading Bot Settings
BOT_COMPOUNDING_ENABLED=true
BOT_MIN_TRADES=1
BOT_MAX_TRADES=10
BOT_TRADING_HOURS="9:00-17:00"

# P2P Transfer Settings
P2P_ENABLED=true
MIN_TRANSFER=10.00
MAX_TRANSFER=10000.00
TRANSFER_FEE=1.00

# Social Media Links
FACEBOOK_URL=https://facebook.com/yourpage
TWITTER_URL=https://twitter.com/yourprofile
TELEGRAM_URL=https://telegram.me/yourchannel

# Payment Gateway Settings
STRIPE_KEY=your_stripe_key
PAYPAL_CLIENT_ID=your_paypal_client_id

# SMS Settings for OTP
TWILIO_SID=your_twilio_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=+11234567890

# File Upload Settings
FILE_UPLOAD_MAX_SIZE=2048
RATE_LIMIT_MAX_REQUESTS=100
